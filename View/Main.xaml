<Page x:Class="PR43.View.Main"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:PR43.View"
      xmlns:Element="clr-namespace:PR43.View.Books"
      mc:Ignorable="d" 
      Title="Main">

    <Grid Margin="10, 5, 10, 5">
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="3*"/>
            <ColumnDefinition Width="7*"/>
        </Grid.ColumnDefinitions>
        <Grid Grid.Column="0">
            <ListView BorderThickness="0" x:Name="TabParent">
                
            </ListView>
        </Grid>
        <Grid Grid.Column="1">
            <Frame x:Name="listFrame" HorizontalContentAlignment="Stretch" VerticalContentAlignment="Stretch" NavigationUIVisibility="Hidden"/>
        </Grid>
    </Grid>
</Page>
